{
  "name": "狗勾挖評論",
  "manifest_version": 3,
  "version": "1.0.0.0",
  "description": "知名搜尋引擎店家評論分析",
  "action": {
    "default_title": "Click to open side panel"
  },
  "permissions": [
    "sidePanel",
    "storage",
    "scripting",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "side_panel": {
    "default_path": "initialSidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}